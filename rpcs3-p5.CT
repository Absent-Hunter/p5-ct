local BASE = 0x400000000
local LOWER, UPPER = BASE + 0x30000000, BASE + 0x40000000

function srchEnemies(as, sr)
  -- search for enemy pointer
  local ms = createMemScan()
  ms.firstScan(soExactValue, vtByteArray, nil, as, nil, LOWER, UPPER, "", 2, 0,
    true, nil, nil, nil, nil)
  ms.waitTillDone()

  -- grab search results
  local r = createFoundList(ms)
  r.initialize()
  if r.Count == 0 then return end

  -- go over search results
  for i = 0, r.Count - 1 do
    -- get pointer destination
    local dest = be4.byteTableToValue(readBytes(tonumber(r[i], 16) + 0x04, 4, true))

    -- get combatant type from the pointer destination (i.e. possible enemy entry)
    local type = readBytes(BASE + dest + 0x05, 1)

    -- check for a valid enemy entry, add to list if valid
    -- encountered values: 1 (party), 2 (enemy), 3 (???)
    if type == 0x02 then
      local rec = mrClone(sr, BASE + dest)
      rec.Description = string.format("Enemy %s -&gt; %X", r[i]:sub(-8), dest)
      rec.DontSave = dontSaveEnemy
      rec.appendToEntry(memrec)
    end
  end

  -- trigger gc
  r.destroy()
  ms.destroy()
end

