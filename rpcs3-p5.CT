    cm[j + 0x8060], cmr[utf8.char(j)] = utf8.char(j), j + 0x8060

    local o = j &lt; 0x60 and 0xEFBC60 or 0xEFBD20
    em[j + o], emr[utf8.char(j)] = utf8.char(j), j + o
  end
end

